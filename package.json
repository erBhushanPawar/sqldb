{
  "name": "@bhushanpawar/sqldb",
  "version": "1.0.20",
  "description": "An intelligent MariaDB client with Redis-backed caching, automatic schema discovery, relationship mapping, and smart cache invalidation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sqldb": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "usage": "ts-node examples/usage.ts",
    "perf": "ts-node examples/usage.ts perf",
    "search": "ts-node examples/search-example.ts",
    "search:api": "ts-node examples/search-example.ts --api",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "mocha -r ts-node/register tests/performance.test.ts --timeout 120000",
    "test:perf": "npm run test:performance"
  },
  "keywords": [
    "mariadb",
    "mysql",
    "redis",
    "cache",
    "database",
    "query-cache",
    "performance",
    "orm",
    "schema-discovery",
    "smart-cache",
    "sqldb"
  ],
  "author": "Bhushan Pawar <bhushanp.cz@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "mariadb": "^3.0.0"
  },
  "dependencies": {
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "chai": "^4.3.0",
    "dotenv": "^16.0.0",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "mariadb": "^3.4.5",
    "mocha": "^10.0.0",
    "mysql2": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erBhushanPawar/sqldb"
  },
